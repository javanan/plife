<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/views/layouts/">

	<excludes>
		<pattern>/assets/*</pattern>
		<pattern>/api/*</pattern>
		<pattern>/upload/*</pattern>
		<pattern>/index.jsp</pattern>
		<pattern>*/login</pattern>
		<pattern>/logout</pattern>
		<pattern>/sys/index</pattern>
	</excludes>
	<decorator name="default" page="default.jsp">

		<pattern>/sys/*</pattern>

	</decorator>
	
</decorators>
